{
    "name": "antoniputra/asmoyo",
    "description": "CMS based laravel framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoni Putra",
            "email": "akiddcode@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "intervention/image": "2.*",
        "intervention/imagecache": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Antoniputra\\Asmoyo": "src/"
        },
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/seeds"
        ]
    },
    "minimum-stability": "stable"
}
